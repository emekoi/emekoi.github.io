{ "version"       : "https://www.jsonfeed.org/version/1.1"
, "title"         : "{{ site.tile }}"
, "home_page_url" : "{{ site.url }}"
, "feed_url"      : "{{ site.url }}/{{ url }}"
, "description"   : "{{ site.description }}"
, "authors"       :
  [ { "name" : "{{ site.author }}"
    , "url"  : "mailto:{{ site.email }}"
    }
  ]
  , "items": [ {{ body }} ]
}
